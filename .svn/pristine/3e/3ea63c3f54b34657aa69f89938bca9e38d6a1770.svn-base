<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data class="com.augurit.agmobile.gzpssb.ItemFGDrainageDataHouse">

        <variable
            name="houseList"
            type="com.augurit.agmobile.gzpssb.bean.SewerageItemBean.HouseListBean" />


        <variable
            name="unitlist"
            type="com.augurit.agmobile.gzpssb.bean.SewerageItemBean.UnitListBean" />

        <variable
            name="poplist"
            type="com.augurit.agmobile.gzpssb.bean.SewerageItemBean.PopListBean" />

        <variable
            name="RoomUnitList"
            type="com.augurit.agmobile.gzpssb.bean.SewerageRoomClickItemBean.UnitListBean"/>
    </data>

    <LinearLayout
        android:id="@+id/ll_sewera_room_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/press_change_bg"
        android:layout_margin="1dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ll_result_item_root"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="8dp">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="8dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/address_item_seweragecompany1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:maxLines="2"
                        android:ellipsize="marquee"
                        android:scrollbars="vertical"
                        android:singleLine="false"
                        android:text=""
                        android:textColor="@color/dust_grey"
                        android:textSize="@dimen/text_size_second" />
                    <TextView
                        android:id="@+id/tv_right_up"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical|right"
                        android:gravity="center"
                        android:text=""
                        android:textSize="12sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/roomtype_item_seweragecompany1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical|right"
                        android:layout_weight="1"
                        android:text=""
                        android:textColor="#a000"
                        android:textSize="12sp" />
                    <Button
                        android:id="@+id/btn_right_bottom"
                        android:layout_width="70dp"
                        android:layout_height="26dp"
                        android:layout_gravity="center_vertical|right"
                        android:background="@drawable/sel_btn_upload"
                        android:gravity="center"
                        android:textColor="@color/agmobile_white"
                        android:text="巡检"
                        android:textSize="12sp" />

                </LinearLayout>
            </LinearLayout>

        </LinearLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/home_divider_color"
            android:layout_marginTop="3dp"/>
    </LinearLayout>


</layout>