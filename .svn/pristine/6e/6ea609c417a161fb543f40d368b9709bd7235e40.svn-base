package com.augurit.am.fw.cache.img.amimageloader;

/**
 * 图片最终显示在ImageView上的宽高像素
 * Created by Administrator on 2016-07-12.
 */
public class AMOverrideSize {
    //图片宽
    private final int width;
    //图片高
    private final int height;

    public AMOverrideSize(int width, int height) {
        this.width = width;
        this.height = height;
    }

    public int getWidth() {
        return width;
    }

    public int getHeight() {
        return height;
    }
}
