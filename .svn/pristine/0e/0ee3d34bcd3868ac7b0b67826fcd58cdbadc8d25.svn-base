public class SewerageUtil {

    public static String getRoomType(String type){
        String str = null;
        switch (type){
            case "01":
                str = "住宅";
                break;
            case "02":
                str = "教育/医疗/卫生/科研";
                break;
            case "03":
                str = "文化/娱乐/体育";
                break;
            case "04":
                str = "军事";
                break;
            case "11":
                str = "工业/交通/仓储";
                break;
            case "12":
                str = "办公";
                break;
            case "13":
                str = "商业/金融/信息";
                break;
            case "14":
                str = "其他";
                break;
        }
        return str;
    }

}
