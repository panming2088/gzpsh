<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data class="com.augurit.agmobile.gzpssb.TableSewerageTwoData">

    </data>

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/common_rectangle_with_shadow"
            android:orientation="vertical"
            android:paddingBottom="13dp"
            android:paddingTop="13dp">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    style="@style/PatrolUploadKeyTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="行业类别" />
                <TextView
                    android:id="@+id/tv_notice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top"
                    android:layout_marginTop="-4dp"
                    android:text="*"
                    android:visibility="gone"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="@dimen/text_size_third" />
            </LinearLayout>



            <android.support.v7.widget.RecyclerView
                android:id="@+id/rv_table1"
                android:layout_width="match_parent"
                android:layout_margin="15dp"
                android:horizontalSpacing="10dp"
                android:padding="6dp"
                android:verticalSpacing="10dp"
                android:layout_height="wrap_content">
            </android.support.v7.widget.RecyclerView>

            <EditText
                android:id="@+id/edt_other_two"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:textSize="14sp"
                android:maxLength="50"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"/>
   <!--         <com.augurit.agmobile.gzps.common.widget.MyGridView
                android:id="@+id/rv_table1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:horizontalSpacing="10dp"
                android:numColumns="3"
                android:padding="6dp"
                android:verticalSpacing="10dp" />-->
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_eventtype"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:background="@drawable/common_rectangle_with_shadow"
            android:orientation="vertical"
            android:paddingBottom="13dp"
            android:paddingTop="13dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    style="@style/PatrolUploadKeyTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="单位类型" />
                <TextView
                    android:id="@+id/tv_notice2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top"
                    android:layout_marginTop="-4dp"
                    android:text="*"
                    android:visibility="gone"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="@dimen/text_size_third" />
            </LinearLayout>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/rv_table2"
                android:layout_below="@+id/rv_table2"
                android:layout_width="match_parent"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="2dp"
                android:horizontalSpacing="10dp"
                android:paddingBottom="6dp"
                android:paddingRight="6dp"
                android:paddingTop="6dp"
                android:paddingEnd="1dp"
                android:verticalSpacing="10dp"
                android:layout_height="wrap_content">
            </android.support.v7.widget.RecyclerView>

            <EditText
                android:id="@+id/edt_other_two1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:textSize="14sp"
                android:maxLength="50"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"/>
        </LinearLayout>

    </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</layout>