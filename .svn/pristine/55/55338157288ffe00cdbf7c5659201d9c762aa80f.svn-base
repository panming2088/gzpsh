public class SpatialBufferParam {

    String distance;
    List<String> geometry;
    List<SpatialQueryParam> layerParams;

    public String getDistance() {
        return distance;
    }

    public void setDistance(String distance) {
        this.distance = distance;
    }

    public List<String> getGeometry() {
        return geometry;
    }

    public void setGeometry(List<String> geometry) {
        this.geometry = geometry;
    }

    public List<SpatialQueryParam> getLayerParams() {
        return layerParams;
    }

    public void setLayerParams(List<SpatialQueryParam> layerParams) {
        this.layerParams = layerParams;
    }
}
