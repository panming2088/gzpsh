                            <RadioButton
                                android:id="@+id/rb_table_sewerageone_four_no"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="无" />

                            <RadioButton
                                android:id="@+id/rb_table_sewerageone_four_yes"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="有" />
                        </RadioGroup>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_takephoto_sewerageone_four"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <com.augurit.agmobile.gzps.common.widget.TextItemTableItem
                            android:id="@+id/tv_sewerage_one_code3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            app:textViewName="许可证编码" />

                        <com.augurit.agmobile.gzpssb.common.view.MultiTakePhotoTableItem
                            android:id="@+id/takephoto_sewerageone_four"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:photoText="排污许可证" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/common_rectangle_with_shadow"
