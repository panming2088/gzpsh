            app:behavior_hideable="false"
            app:layout_behavior="@string/bottom_sheet_behavior_with_anchor">

            <include layout="@layout/audit_opinion_layout" />
        </LinearLayout>
    </android.support.design.widget.CoordinatorLayout>
