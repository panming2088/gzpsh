package com.augurit.agmobile.gzps.common.baiduselectlocation.model;

import com.esri.core.geometry.Geometry;

import java.io.Serializable;

/**
 * Created by xcl on 2017/12/1.
 */

public class BaiduSelectLocationModel  implements Serializable {

    private Geometry lastSelectLocation;
    private Geometry facilityOriginLocation;
    private String lastSelectAddress;
    private double initScale;
    private boolean ifReadOnly;

    public Geometry getLastSelectLocation() {
        return lastSelectLocation;
    }

    public void setLastSelectLocation(Geometry lastSelectLocation) {
        this.lastSelectLocation = lastSelectLocation;
    }

    public Geometry getFacilityOriginLocation() {
        return facilityOriginLocation;
    }

    public void setFacilityOriginLocation(Geometry facilityOriginLocation) {
        this.facilityOriginLocation = facilityOriginLocation;
    }

    public double getInitScale() {
        return initScale;
    }

    public void setInitScale(double initScale) {
        this.initScale = initScale;
    }

    public String getLastSelectAddress() {
        return lastSelectAddress;
    }

    public void setLastSelectAddress(String lastSelectAddress) {
        this.lastSelectAddress = lastSelectAddress;
    }

    public boolean isIfReadOnly() {
        return ifReadOnly;
    }

    public void setIfReadOnly(boolean ifReadOnly) {
        this.ifReadOnly = ifReadOnly;
    }
}
