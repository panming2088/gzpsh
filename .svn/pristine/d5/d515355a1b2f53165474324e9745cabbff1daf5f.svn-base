                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginLeft="15dp"
                android:background="@drawable/round_blue_rectangle"
                android:gravity="center"
                android:padding="4dp"
                android:text="无法挂接"
                android:textColor="@color/agmobile_white"
                android:textSize="13sp"
                tools:visibility="visible" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/round_blue_rectangle"
            android:orientation="horizontal"
            android:paddingLeft="5dp"
            android:paddingTop="6dp"
            android:paddingRight="5dp"
            android:paddingBottom="6dp"
            android:visibility="gone"
            tools:visibility="visible">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/common_sel_btn_light"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <CheckBox
                    style="@style/AGMobile.Widget.CheckBox.Blue"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:clickable="false"
                    android:textColor="@color/agmobile_white" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="上游"
                    android:textColor="@color/agmobile_white"
                    android:textSize="@dimen/text_size_third" />
            </LinearLayout>

            <View
                android:id="@+id/view_divider"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="6dp"
                android:layout_marginTop="3dp"
                android:layout_marginRight="6dp"
                android:layout_marginBottom="3dp"
                android:background="@color/gray" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:background="@drawable/common_sel_btn_light"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <CheckBox
                    style="@style/AGMobile.Widget.CheckBox.Blue"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:clickable="false"
                    android:textColor="@color/agmobile_white" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="下游"
                    android:textColor="@color/agmobile_white"
                    android:textSize="@dimen/text_size_third" />
            </LinearLayout>
        </LinearLayout>

        <TextView
