    <LinearLayout
        android:id="@+id/ll_check_hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:visibility="gone"
        tools:visibility="visible">

        <TextView
            android:id="@+id/tv_check_person"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:text="欧弟"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/iv_phone"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@mipmap/ic_phone"
            android:visibility="gone"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/tv_check_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:textColor="@color/agmobile_blue"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_check_hint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:text="@string/is_checking_if_has_been_uploaded_before"
            android:textSize="14sp" />

