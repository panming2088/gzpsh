<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="false"
    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <com.esri.android.map.MapView
            android:id="@+id/map"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <com.augurit.agmobile.patrolcore.editmap.widget.LocationMarker
            android:id="@+id/locationMarker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:visibility="gone" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <include layout="@layout/common_patrol_toolbar" />

                <LinearLayout
                    android:id="@+id/ll_topbar_container"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_gravity="center_horizontal"
                    android:orientation="vertical"
                    android:visibility="gone" />

                <LinearLayout
                    android:id="@+id/ll_question_topbar_container"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_gravity="center_horizontal"
                    android:orientation="vertical"
                    android:visibility="gone" />

            </FrameLayout>


            <LinearLayout
                android:id="@+id/ll_layer_url_init_fail"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal"
                android:background="@color/layer_url_init_fail_msg_color"
                android:orientation="horizontal"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_gravity="right|center_vertical"
                        android:src="@mipmap/ic_alert_yellow" />

                </LinearLayout>


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_gravity="left|center_vertical"
                        android:gravity="center"
                        android:text="@string/layer_url_init_fail_msg" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_query_layer_grid"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@color/agmobile_white"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:id="@+id/show_all_layer"
                    android:layout_width="60dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:text="选取类别" />

                <TextView
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="8dp"
                    android:background="@color/agmobile_grey_1"
                    android:gravity="center" />

                <GridView
                    android:id="@+id/gridview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:numColumns="4" />

            </LinearLayout>

            <RelativeLayout
                android:id="@+id/rl_btn_add"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:gravity="right">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="40dp">

                    <Button
                        android:id="@+id/btn_add"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:background="@mipmap/icon_jbj_cancel" />

                    <Button
                        android:id="@+id/btn_save"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:background="@drawable/ic_action_save"
                        android:visibility="gone" />

                    <Button
                        android:id="@+id/btn_cancel"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:background="@mipmap/icon_jbj_selected"
                        android:visibility="gone" />

                    <Button
                        android:id="@+id/btn_delete"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:background="@drawable/ic_action_discard"
                        android:visibility="gone" />

                    <Button
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:background="@drawable/ic_action_undo"
                        android:visibility="gone" />

                </LinearLayout>

            </RelativeLayout>
            <LinearLayout
                android:id="@+id/ll_jhj"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:gravity="right"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="16dp"
                    android:orientation="vertical"
                    android:scaleType="fitCenter">

                    <ImageView
                        android:id="@+id/btn_add_jhj"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:src="@mipmap/icon_jhj_cancel" />

                    <ImageView
                        android:id="@+id/btn_jhj_cancel"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:src="@mipmap/icon_jhj_selected"
                        android:visibility="gone" />

                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_search_menpai"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginRight="16dp"
                    android:orientation="vertical"
                    android:scaleType="fitCenter">

                    <ImageView
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:src="@mipmap/ic_search1" />

                </LinearLayout>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_btn_edit"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="16dp"
                    android:orientation="vertical"
                    android:scaleType="fitCenter"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/btn_edit"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:src="@mipmap/icon_facitily_delmoj_normal" />

                    <ImageView
                        android:id="@+id/btn_edit_cancel"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:src="@mipmap/icon_facitily_delmoj_selection"
                        android:visibility="gone" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_check"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:orientation="vertical"
                        android:scaleType="fitCenter">

                        <ImageView
                            android:id="@+id/btn_check"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/icon_yinjing_line_normal" />

                        <ImageView
                            android:id="@+id/btn_check_cancel"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/icon_yinjing_line_selection"
                            android:visibility="gone" />

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_edit_pipe"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:gravity="right"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:orientation="vertical"
                        android:scaleType="fitCenter"
                        android:visibility="gone">

                        <ImageView
                            android:id="@+id/btn_edit_pipe"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/icon_pipe_del_normal" />

                        <ImageView
                            android:id="@+id/btn_pipe_cancel"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/icon_pipe_del_selection"
                            android:visibility="gone" />

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_zhiyi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:gravity="right"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:orientation="vertical"
                        android:scaleType="fitCenter">

                        <ImageView
                            android:id="@+id/btn_edit_zhiyi"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/icon_doubt_normal" />

                        <ImageView
                            android:id="@+id/btn_zhiyi_cancel"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/icon_doubt_selection"
                            android:visibility="gone" />

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/ll_dy"
                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:orientation="vertical"
                        android:scaleType="fitCenter">

                        <ImageView
                            android:id="@+id/btn_edit_dy"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/icon_facitily_dygj_normal" />

                        <ImageView
                            android:id="@+id/btn_dy_cancel"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/icon_facitily_dygj_selection"
                            android:visibility="gone" />

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:gravity="right"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:orientation="vertical"
                        android:scaleType="fitCenter">

                        <ImageView
                            android:id="@+id/btn_edit_yj"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/ic_edit_pipe" />

                        <ImageView
                            android:id="@+id/btn_yj_cancel"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:src="@mipmap/ic_cancel_add_new_facility"
                            android:visibility="gone" />

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/btn_layer"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="12dp"
                    android:layout_marginRight="16dp"
                    android:orientation="vertical"
                    android:scaleType="fitCenter">

                    <ImageView
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:src="@mipmap/phone_ic_layers_white_24dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="图层"
                        android:textSize="8sp"
                        android:visibility="gone" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_legend"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="12dp"
                    android:layout_marginRight="16dp"
                    android:orientation="vertical"
                    android:scaleType="fitCenter">

                    <ImageView
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:src="@mipmap/ic_legend" />

                </LinearLayout>


                <LinearLayout
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="12dp"
                    android:visibility="gone"
                    android:layout_marginRight="16dp"
                    android:orientation="vertical"
                    android:scaleType="fitCenter">

                    <ImageView
                        android:id="@+id/btn_swerage_user"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:src="@drawable/drainage_selector_jbjpsdy_map_btn_swerage_user_src" />

                </LinearLayout>


                <LinearLayout
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:visibility="gone"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="12dp"
                    android:layout_marginRight="16dp"
                    android:orientation="vertical"
                    android:scaleType="fitCenter">

                    <ImageView
                        android:id="@+id/btn_drainage_user"
                        android:layout_width="42dp"
                        android:layout_height="42dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:src="@drawable/drainage_selector_jbjpsdy_map_btn_swerage_user_src" />

                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/rl_btn_check"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:gravity="right">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="40dp">

                        <Button
                            android:id="@+id/btn_check_situation"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="16dp"
                            android:background="@mipmap/icon_check_situation_cancel" />

                        <Button
                            android:id="@+id/btn_situation_cancel"
                            android:layout_width="42dp"
                            android:layout_height="42dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="16dp"
                            android:background="@mipmap/icon_check_situation"
                            android:visibility="gone" />
                    </LinearLayout>

                </RelativeLayout>


            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_my_upload_layer"
            android:layout_width="wrap_content"
            android:visibility="gone"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/bg_upload_layer"
            android:elevation="1dp"
            android:gravity="left"
            android:orientation="vertical"
            android:padding="2dp">

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/iv_my_upload_layer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:clickable="false"
                android:switchMinWidth="60dp"
                android:switchPadding="10dp"
                android:typeface="normal" />

            <TextView
                android:id="@+id/tv_my_upload_layer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="4dp"
                android:gravity="center"
                android:text="我的上报"
                android:textColor="@drawable/sel_text_view_upload_layer"
                android:textSize="12sp"
                android:visibility="visible" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_upload_layer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/ll_my_upload_layer"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/bg_upload_layer"
            android:elevation="1dp"
            android:gravity="left"
            android:orientation="vertical"
            android:padding="2dp"
            android:visibility="gone">

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/iv_upload_layer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:clickable="false"
                android:switchMinWidth="60dp"
                android:switchPadding="10dp"
                android:typeface="normal" />

            <TextView
                android:id="@+id/tv_upload_layer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="4dp"
                android:gravity="center"
                android:text="本区数据"
                android:textColor="@drawable/sel_text_view_upload_layer"
                android:textSize="12sp"
                android:visibility="visible" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_compass_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/ll_upload_layer"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:orientation="vertical" />

        <LinearLayout
            android:id="@+id/ll_tool_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/ll_compass_container"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:background="@color/transparent"
            android:elevation="1dp"
            android:gravity="left"
            android:orientation="vertical"
            android:padding="2dp"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/ll_question_tool_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/ll_compass_container"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:background="@color/transparent"
            android:elevation="1dp"
            android:gravity="left"
            android:orientation="vertical"
            android:padding="2dp"
            android:visibility="gone" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="210dp"
            android:background="@color/transparent"
            android:orientation="vertical"
            android:visibility="visible" />


        <LinearLayout
            android:id="@+id/ll_rotate_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="150dp"
            android:orientation="vertical"
            android:visibility="gone" />

        <com.augurit.agmobile.patrolcore.common.widget.LocationButton
            android:id="@+id/locationButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="50dp" />

        <com.augurit.agmobile.mapengine.common.widget.scaleview.MapScaleView
            android:id="@+id/scale_view"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="16dp"
            android:layout_marginBottom="16dp" />

        <!--<LinearLayout-->
        <!--android:id="@+id/ll_legend"-->
        <!--android:layout_width="32dp"-->
        <!--android:layout_height="32dp"-->
        <!--android:layout_alignParentRight="true"-->
        <!--android:layout_marginRight="24dp"-->
        <!--android:layout_marginTop="200dp"-->
        <!--android:background="@drawable/round_bg_blue"-->
        <!--android:backgroundTint="@color/agmobile_white"-->
        <!--android:elevation="6dp"-->
        <!--android:gravity="center">-->

        <!--<ImageView-->

        <!--android:layout_width="18dp"-->
        <!--android:layout_height="18dp"-->
        <!--android:src="@mipmap/ic_legend" />-->
        <!--</LinearLayout>-->
        <LinearLayout
            android:id="@+id/ll_component_list"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:layout_alignParentBottom="true"
            android:background="@color/agmobile_white"
            android:orientation="vertical"
            android:visibility="gone">

        </LinearLayout>
    </RelativeLayout>

    <View
        android:id="@+id/ll_bottm"
        android:layout_width="match_parent"
        android:layout_height="220dp"
        android:layout_below="@id/btn_login" />

    <LinearLayout
        android:id="@+id/check_legend_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|left"
        android:layout_margin="10dp"
        android:background="@color/agmobile_white_dark"
        android:clickable="true"
        android:orientation="vertical"
        android:visibility="gone">

        <include layout="@layout/check_legeng_view" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/map_bottom_sheet"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:background="@color/agmobile_white_dark"
        android:clickable="true"
        android:orientation="vertical"
        android:visibility="gone"
        app:behavior_hideable="false"
        app:behavior_peekHeight="40dp"
        app:layout_behavior="@string/bottom_sheet_behavior_with_anchor">

        <include layout="@layout/upload_map_hook_layout" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/dis_map_bottom_sheet"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/agmobile_white_dark"
        android:clickable="true"
        android:orientation="vertical"
        android:visibility="gone"
        app:behavior_hideable="false"
        app:behavior_peekHeight="40dp"
        app:layout_behavior="@string/bottom_sheet_behavior_with_anchor">

        <include layout="@layout/review_stream_container" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/jhjview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/agmobile_white_dark"
        android:clickable="true"
        android:orientation="vertical"
        android:visibility="gone"
        app:behavior_hideable="false"
