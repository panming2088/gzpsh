<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="100" android:exitFadeDuration="@android:integer/config_mediumAnimTime">

    <!--禁用时背景，需要注意item顺序-->
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="@color/agmobile_grey_0" />
            <size android:width="90dp" android:height="90dp" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="@color/agmobile_blue" />
            <size android:width="90dp" android:height="90dp" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/agmobile_blue_dark" />
            <stroke android:color="@color/agmobile_blue" />
            <size android:width="90dp" android:height="90dp" />
        </shape>
    </item>
</selector>