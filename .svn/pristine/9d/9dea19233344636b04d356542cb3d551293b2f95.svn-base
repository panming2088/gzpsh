package com.augurit.agmobile.gzps.im.bean;

/**
 * 此类是重构时新加,Friend,Groups,GroupMember共同的父类
 * 对于Groups,最开始设计时有groupsId的属性
 * 修改后UserInfoBean的id对应为Groups中的groupsId
 * 这种有些不伦不类的折中修改主要是为了尽量少修改现有代码
 */
public class UserInfoBean {
    private FriendItem result;

    public FriendItem getResult() {
        return result;
    }

    public void setResult(FriendItem result) {
        this.result = result;
    }

}
