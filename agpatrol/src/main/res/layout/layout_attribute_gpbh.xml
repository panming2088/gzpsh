<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:clickable="false"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <LinearLayout
            android:id="@+id/ll_tv1"
            style="@style/PatrolUploadKeyTextStyle"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:clickable="false"
            android:gravity="center_vertical">
            <!--文本框 -->
            <TextView
                android:id="@+id/tv_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="已挂牌编号"
                android:textColor="#FF666666"
                android:textSize="@dimen/partrol_upload_text_size" />

            <TextView
                android:id="@+id/tv_requiredTag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:layout_marginTop="0dp"
                android:text="*"
                android:textColor="@android:color/holo_red_light"
                android:textSize="@dimen/text_size_third"
                android:visibility="visible" />
        </LinearLayout>

        <CheckBox
            android:id="@+id/cb_yes"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:checked="true"
            android:gravity="center_vertical"
            android:text="有" />

        <EditText
            android:id="@+id/et_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/patrol_upload_view_margin_left"
            android:layout_marginRight="8dp"
            android:maxLength="20"
            android:hint=""
            android:visibility="visible" />


    </LinearLayout>
    <com.augurit.agmobile.gzpssb.common.view.MultiTakePhotoTableItem
        android:visibility="gone"
        android:id="@+id/take_photo_well"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:photoText="添加照片" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            style="@style/PatrolUploadKeyTextStyle"
            android:layout_height="match_parent"
            android:layout_centerVertical="false"
            android:clickable="false" />

        <CheckBox
            android:id="@+id/cb_no"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="无" />


    </LinearLayout>

    <!--android:textColor="@drawable/sel_text_item_color"-->
</LinearLayout>
