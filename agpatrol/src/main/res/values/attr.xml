<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomBottomSheetBehavior">
        <attr name="anchorPoint" format="dimension"/>
    </declare-styleable>

    <!--圆形头像-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <!--圆角头像-->
    <declare-styleable name="SelectableRoundedImageView">
        <attr name="sriv_left_top_corner_radius" format="dimension" />
        <attr name="sriv_right_top_corner_radius" format="dimension" />
        <attr name="sriv_left_bottom_corner_radius" format="dimension" />
        <attr name="sriv_right_bottom_corner_radius" format="dimension" />
        <attr name="sriv_border_width" format="dimension" />
        <attr name="sriv_border_color" format="color" />
        <attr name="sriv_oval" format="boolean" />
        <attr name="android:scaleType" />
    </declare-styleable>

    <declare-styleable name="SwitchButton">
        <attr name="kswThumbDrawable" format="reference"/>
        <attr name="kswThumbColor" format="color|reference"/>
        <attr name="kswThumbMargin" format="dimension|reference"/>
        <attr name="kswThumbMarginTop" format="dimension|reference"/>
        <attr name="kswThumbMarginBottom" format="dimension|reference"/>
        <attr name="kswThumbMarginLeft" format="dimension|reference"/>
        <attr name="kswThumbMarginRight" format="dimension|reference"/>
        <attr name="kswThumbWidth" format="dimension|reference"/>
        <attr name="kswThumbHeight" format="dimension|reference"/>
        <attr name="kswThumbRadius" format="dimension|reference"/>
        <attr name="kswBackRadius" format="dimension|reference"/>
        <attr name="kswBackDrawable" format="reference"/>
        <attr name="kswBackColor" format="color|reference"/>
        <attr name="kswFadeBack" format="boolean"/>
        <attr name="kswBackMeasureRatio" format="float"/>
        <attr name="kswAnimationDuration" format="integer"/>
        <attr name="kswTintColor" format="color|reference"/>
        <attr name="kswTextOn" format="string"/>
        <attr name="kswTextOff" format="string"/>
        <attr name="kswTextMarginH" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TextItemTableItem">
        <attr name="textViewName" format="string"/>
        <attr name="editTextHint" format="string"/>
        <attr name="editTextMaxLength" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TextFieldTableItem">
        <attr name="fieldtextViewName" format="string"/>
        <attr name="fieldeditTextHint" format="string"/>
        <attr name="fieldeditMaxLength" format="integer"/>
    </declare-styleable>

    <attr name="spinnertextViewName" format="string"/>
    <attr name="data" format="reference"/>
    <declare-styleable name="SpinnerTableItem">
        <attr name="spinnertextViewName"/>
        <attr name="data"/>
    </declare-styleable>

    <declare-styleable name="SpinnerTableItem2">
        <attr name="spinnertextViewName"/>
        <attr name="data"/>
    </declare-styleable>


    <declare-styleable name="TakePhotoTableItem">
        <attr name="photoText" format="string"/>
    </declare-styleable>

    <declare-styleable name="MultiSelectTableItlem">
        <attr name="multiselecttextViewName" format="string"/>
    </declare-styleable>

    <!--竖向viewpager-->
    <declare-styleable name="YViewPager">
        <attr name="orientation">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
        </attr>
        <attr name="circulatory" format="boolean"/>
        <attr name="circulationCount" format="integer"/>

    </declare-styleable>

    <declare-styleable name="FileCustomRecyclerView">
        <attr name="file_deleteDrawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="AGFilePicker">
        <attr name="FilePickerTextViewName" format="string"/>
    </declare-styleable>

    <declare-styleable name="ProblemTypeView">
        <attr name="ptv_title" format="string"/>
        <attr name="ptv_title_color" format="color"/>
        <attr name="ptv_need_photo" format="boolean"/>
        <attr name="ptv_max_photo" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TimePickerItem">
        <attr name="tpi_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="SelectFacilityView">
        <attr name="sfv_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="MaxHeightLinearLayout">
        <attr name="mhll_height_ratio" format="reference|float"/>
        <attr name="mhll_height_dimen" format="reference|dimension"/>
    </declare-styleable>

</resources>